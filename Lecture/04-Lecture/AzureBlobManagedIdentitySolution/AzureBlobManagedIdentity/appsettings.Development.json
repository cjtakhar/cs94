{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "PictureSettings": {
    "PictureContainerName": "pictures"
  },
  // Replace ContainerEndPoint with your storage account and TenantId with your TenantId 
  // from Microsoft Entra Id overview page
  "StorageAccountSettings": {
    "ContainerEndpoint": "https://<your storage account>.blob.core.windows.net/{0}",
    "Url": "https://<your storage account>.blob.core.windows.net/",
    "TenantId": "<your tenant id>"
  }
}
