{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "QueueSettings": {
    "StringQueueConnectionString": "https://<your storage account>.queue.core.windows.net/myqueue-items",
    "JsonQueueConnectionString": "https://<your storage account>.queue.core.windows.net/myjsonqueue-items"
  },
  "LimitSettings": {
    "DefaultVisibilityInSeconds": 60,
    "DefaultTimeToLiveInSeconds": 180
  }
}
