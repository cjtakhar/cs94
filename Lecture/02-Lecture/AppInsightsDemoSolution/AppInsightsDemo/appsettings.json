{
  "AllowedHosts": "*",
  "SnapshotCollectorConfiguration": {
    "IsEnabledInDeveloperMode": false,
    "ThresholdForSnapshotting": 1,
    "MaximumSnapshotsRequired": 10,
    "MaximumCollectionPlanSize": 100,
    "ReconnectInterval": "00:15:00",
    "ProblemCounterResetInterval": "0.00:10:00",
    "SnapshotsPerTenMinutesLimit": 10,
    "SnapshotsPerDayLimit": 100,
    "SnapshotInLowPriorityThread": true,
    "ProvideAnonymousTelemetry": false,
    "FailedRequestLimit": 200
  },
  "ApplicationInsights": {
    "EnableAdaptiveSampling": false,
    "DevelopmentMode": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  }
}